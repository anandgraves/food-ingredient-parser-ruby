grammar Root
  include Common
  include List
  include ListColoned

  rule root
    root_prefix? ws* contains:( list_coloned / list ) ws* '.'? ws* <RootNode>
  end

  rule root_prefix
    (
      'ingredients'i / 'contains'i /
      ('ingred'i [IÏiï] [EËeë] 'n'i ( 't'i 'en'i? 'declaratie'i? )? ) / 'bevat'i / 'dit zit er in'i / 'samenstelling'i /
      'zutaten'i
    ) ( ws* [:;.] / ws ) ws*
  end

end
